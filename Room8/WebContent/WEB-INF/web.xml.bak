<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>UserSession</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>updateInfo</servlet-name>
    <servlet-class>UpdateDate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>updateInfo</servlet-name>
    <url-pattern>/UpdateDate</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>/pages/Default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <servlet-name>Booking</servlet-name>
    <servlet-class>Booking</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Booking</servlet-name>
    <url-pattern>/Booking</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ScheduleRoomHour</display-name>
    <servlet-name>ScheduleRoomHour</servlet-name>
    <servlet-class>ScheduleRoomHour</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ScheduleRoomHour</servlet-name>
    <url-pattern>/ScheduleRoomHour</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Amenities</display-name>
    <servlet-name>Amenities</servlet-name>
    <servlet-class>Amenities</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Amenities</servlet-name>
    <url-pattern>/Amenities</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AddEditREmoveRooms</display-name>
    <servlet-name>AddEditREmoveRooms</servlet-name>
    <servlet-class>AddEditREmoveRooms</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddEditREmoveRooms</servlet-name>
    <url-pattern>/AddEditREmoveRooms</url-pattern>
  </servlet-mapping>
</web-app>
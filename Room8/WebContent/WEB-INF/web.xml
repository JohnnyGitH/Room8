<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
  <servlet>
    <servlet-name>login</servlet-name>
		<servlet-class>codebehind.UserSession</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>updateInfo</servlet-name>
    <servlet-class>UpdateDate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>updateInfo</servlet-name>
    <url-pattern>/UpdateDate</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>/pages/Default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <servlet-name>Booking</servlet-name>
		<servlet-class>codebehind.Booking</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Booking</servlet-name>
    <url-pattern>/Booking</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ScheduleRoomHour</display-name>
    <servlet-name>ScheduleRoomHour</servlet-name>
    <servlet-class>codebehind.ScheduleRoomHour</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ScheduleRoomHour</servlet-name>
    <url-pattern>/ScheduleRoomHour</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Amenities</display-name>
    <servlet-name>Amenities</servlet-name>
    <servlet-class>codebehind.Amenities</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Amenities</servlet-name>
    <url-pattern>/Amenities</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AddEditREmoveRooms</display-name>
    <servlet-name>AddEditREmoveRooms</servlet-name>
    <servlet-class>codebehind.AddEditREmoveRooms</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddEditREmoveRooms</servlet-name>
    <url-pattern>/AddEditREmoveRooms</url-pattern>
  </servlet-mapping>
	<servlet>
		<description></description>
		<servlet-name>PopUp</servlet-name>
		<servlet-class>codebehind.PopUp</servlet-class>

	</servlet>
	<servlet-mapping>
		<servlet-name>PopUp</servlet-name>
		<url-pattern>/PopUp</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>editUser</display-name>
		<servlet-name>editUser</servlet-name>
		<servlet-class>codebehind.EditUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>editUser</servlet-name>
		<url-pattern>/editUser</url-pattern>
	</servlet-mapping>
</web-app>